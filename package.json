{
  "name": "initialsetup",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "build": "webpack --config webpack.config.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "css-build": "yarn run css-build-clean && yarn run css-build-sass && yarn run css-build-autoprefix && yarn run css-build-cleancss",
    "css-build-autoprefix": "postcss --use autoprefixer --map false --output static/assets/bundles/css/main.css static/assets/bundles/css/main.css",
    "css-build-cleancss": "cleancss -o static/assets/bundles/css/main.min.css static/assets/bundles/css/main.css",
    "css-build-clean": "rimraf css",
    "css-build-sass": "node-sass --output-style expanded --source-map true src/scss/main.scss static/assets/bundles/css/main.css --importer=node_modules/node-sass-tilde-importer",
    "css-deploy": "yarn run css-build",
    "css-start": "yarn run css-build-sass --watch"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/history": "^4.7.5",
    "@types/invariant": "^2.2.31",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^5.1.4",
    "@types/redux": "^3.6.0",
    "@types/webpack-env": "^1.15.1",
    "axios": "^0.21.1",
    "connected-react-router": "^6.8.0",
    "history": "^4.10.1",
    "invariant": "^2.2.4",
    "node-sass": "^4.13.1",
    "node-sass-tilde-importer": "^1.0.2",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux-dynamic-modules": "^5.2.3",
    "redux-dynamic-modules-thunk": "^5.2.3",
    "redux-thunk": "^2.3.0",
    "source-map": "^0.7.3",
    "tslint": "^6.1.1",
    "tslint-react": "^4.2.0",
    "webpack-dev-server": "^3.10.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
